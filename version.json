    "^refs/heads/unorel/winui/\\d+\\.\\d+" // we also release branches starting with rel/winui/N.N.N
  ],
  "nugetPackageVersion": {
    "semVer": 2
  },
  "cloudBuild": {
    "buildNumber": {
      "enabled": false
    }
  }
}
